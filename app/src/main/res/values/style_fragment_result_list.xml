<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="FragmentResultListNoContent" parent="WrapContent">
        <item name="android:textSize">@dimen/title</item>
        <item name="android:color">@color/gray</item>
    </style>

    <style name="FragmentResultListMore" parent="MatchParentWrapContent">
        <item name="android:padding">5dp</item>

        <item name="android:textSize">@dimen/subhead</item>
        <item name="android:textColor">@color/white</item>

        <item name="android:background">@color/pink</item>
    </style>

    <style name="FragmentResultList" parent="MatchParentWrapContent">
        <item name="android:paddingLeft">@dimen/vertical_layout_margin</item>
        <item name="android:paddingRight">@dimen/vertical_layout_margin</item>
    </style>
</resources>